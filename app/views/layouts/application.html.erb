<!DOCTYPE html>
<html>
<head>
  <title>Day12</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:notice] %>
	<h3 id="notice"><%= flash[:notice]%></h3>
<% end %>	

<% if flash[:error]%></h3>
	<h3 id="error"><%= flash[:error]%></h3>
<% end %>
<%= yield %>

</body>
</html>
