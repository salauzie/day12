<%= form_for @clinic do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :facility_type %>
	<%= f.text_field :facility_type %>
	<br>
	<%= f.label :address %>
	<%= f.text_field :address %>
	<br>
	<%= f.label :city %>
	<%= f.text_field :city %>
	<br>
	<%= f.label :state %>
	<%= f.text_field :state %>
	<br>
	<% @doctors.each do |d| %>
		<%=check_box_tag 'clinic[doctor_ids][]', d.id, @clinic.doctors.include?(d) %>
		<%= d.name %>
	<% end %>	

	<br>
	<br>
	<%= f.submit %>
	<% end %>